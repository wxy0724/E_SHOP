<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atguigu.mapper.SkuMapper">
	<insert id="insertSku" parameterType="com.atguigu.bean.T_MALL_SKU"
		useGeneratedKeys="true" keyColumn="id" keyProperty="id">
		insert into
		t_mall_sku ( shp_id, kc, jg, sku_mch, kcdz )
		values( #{shp_id}, #{kc}, #{jg}, #{sku_mch}, #{kcdz} )
	</insert>

	<insert id="insertsku_attr" parameterType="map">
		insert into t_mall_sku_attr_value 
		(shxm_id,shxzh_id, shp_id,sku_id)values
		<foreach collection="list" item="t_sku_attr_value" separator=",">
		(#{t_sku_attr_value.shxm_id},#{t_sku_attr_value.shxzh_id},#{shp_id},#{sku_id})
		</foreach>
	</insert>

</mapper>